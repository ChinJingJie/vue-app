{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentDate: new Date().getUTCDate(),\n      intMonth: new Date().getMonth() + 1,\n      currentYear: new Date().getFullYear(),\n      days: ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat']\n    };\n  },\n\n  methods: {\n    daysInMonth() {\n      return new Date(this.currentYear, this.intMonth, 0).getDate();\n    },\n\n    startDay() {\n      return new Date(this.currentYear, this.intMonth - 1, 1).getDay();\n    },\n\n    next() {\n      if (this.intMonth > 11) {\n        this.intMonth = 1;\n        this.currentYear++;\n      } else {\n        this.intMonth++;\n      }\n    },\n\n    prev() {\n      if (this.intMonth <= 1) {\n        this.intMonth = 12;\n        this.currentYear--;\n      } else {\n        this.intMonth--;\n      }\n    },\n\n    highlightToday(num) {\n      const calenderFullDate = new Date(this.currentYear, this.intMonth - 1, num).toDateString();\n      const currentFullDate = new Date().toDateString();\n      return new Date(this.currentYear, this.intMonth - 1, num) === new Date();\n    }\n\n  },\n  computed: {\n    wordMonth() {\n      return new Date(this.currentYear, this.intMonth - 1).toLocaleString('default', {\n        month: 'long'\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,eAAe;EACbA,IAAI,GAAE;IACJ,OAAM;MACJC,WAAW,EAAE,IAAIC,IAAJ,GAAWC,UAAX,EADT;MAEJC,QAAQ,EAAE,IAAIF,IAAJ,GAAWG,QAAX,KAAwB,CAF9B;MAGJC,WAAW,EAAE,IAAIJ,IAAJ,GAAWK,WAAX,EAHT;MAIJC,IAAI,EAAE,CAAC,KAAD,EAAO,KAAP,EAAa,KAAb,EAAmB,KAAnB,EAAyB,KAAzB,EAA+B,KAA/B,EAAqC,KAArC;IAJF,CAAN;EAMD,CARY;;EASbC,OAAO,EAAC;IACNC,WAAW,GAAE;MACX,OAAO,IAAIR,IAAJ,CAAS,KAAKI,WAAd,EAA0B,KAAKF,QAA/B,EAAyC,CAAzC,EAA4CO,OAA5C,EAAP;IACD,CAHK;;IAINC,QAAQ,GAAE;MACR,OAAO,IAAIV,IAAJ,CAAS,KAAKI,WAAd,EAA0B,KAAKF,QAAL,GAAc,CAAxC,EAA0C,CAA1C,EAA6CS,MAA7C,EAAP;IACD,CANK;;IAONC,IAAI,GAAE;MACJ,IAAI,KAAKV,QAAL,GAAgB,EAApB,EAAuB;QACrB,KAAKA,QAAL,GAAgB,CAAhB;QACA,KAAKE,WAAL;MACD,CAHD,MAGK;QACH,KAAKF,QAAL;MACF;IACD,CAdK;;IAeNW