{"ast":null,"code":"export default {\n  data() {\n    return {\n      newHero: \"\",\n      dcHeros: [{\n        name: 'SuperGirl'\n      }, {\n        name: 'Flash'\n      }, {\n        name: 'Batman'\n      }, {\n        name: 'Arrow'\n      }, {\n        name: 'SuperMan'\n      }]\n    };\n  },\n\n  computed: {\n    herosCount() {\n      return this.dcHeros.length;\n    }\n\n  },\n  methods: {\n    addHero() {\n      if (this.newHero !== \"\") {\n        this.dcHeros.unshift({\n          name: this.newHero\n        });\n        this.newHero = \"\";\n      }\n    },\n\n    remove(index) {\n      this.dcHeros = this.dcHeros.filter((hero, i) => i != index);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmBA,eAAe;EACbA,IAAI,GAAE;IACJ,OAAM;MACJC,OAAO,EAAC,EADJ;MAEJC,OAAO,EAAE,CAAC;QAACC,IAAI,EAAC;MAAN,CAAD,EAAoB;QAACA,IAAI,EAAC;MAAN,CAApB,EAAmC;QAACA,IAAI,EAAC;MAAN,CAAnC,EAAmD;QAACA,IAAI,EAAC;MAAN,CAAnD,EAAkE;QAACA,IAAI,EAAC;MAAN,CAAlE;IAFL,CAAN;EAID,CANY;;EAObC,QAAQ,EAAE;IACRC,UAAU,GAAE;MACV,OAAO,KAAKH,OAAL,CAAaI,MAApB;IACD;;EAHO,CAPG;EAYbC,OAAO,EAAC;IACNC,OAAO,GAAE;MACP,IAAG,KAAKP,OAAL,KAAiB,EAApB,EAAuB;QACrB,KAAKC,OAAL,CAAaO,OAAb,CAAqB;UAACN,IAAI,EAAC,KAAKF;QAAX,CAArB;QACA,KAAKA,OAAL,GAAe,EAAf;MACF;IACD,CANK;;IAONS,MAAM,CAACC,KAAD,EAAO;MACX,KAAKT,OAAL,GAAe,KAAKA,OAAL,CAAaU,MAAb,CAAoB,CAACC,IAAD,EAAMC,CAAN,KAAYA,KAAKH,KAArC,CAAf;IACD;;EATK;AAZK,CAAf","names":["data","newHero","dcHeros","name","computed","herosCount","length","methods","addHero","unshift","remove","index","filter","hero","i"],"sourceRoot":"","sources":["C:\\Users\\cjjie\\Desktop\\projects\\vue-app\\vue3\\src\\components\\DcHeros.vue"],"sourcesContent":["<template>\r\n  <div class=\"m-auto\">\r\n      <h1 class=\"text-2xl text-center m-5 font-bold\">Dc Heros</h1>\r\n      <h2 class=\"py-3 bg-blue-500 text-white font-bold underline text-center text-xl\">A total of {{ herosCount }} heros listed below</h2>\r\n      <ul class=\"bg-blue-300 p-5\">\r\n        <li class=\"flex justify-between\" v-for=\"